   [:div
    [:p "Comments for hashmaps..."]]

   "Lists"
   [:div
    [:p "This is unfair to "
     [:code "get*"]
     ": "
     [:code "clojure.core/get"]
     " always returns "
     [:code "nil"]
     " when given a list, whereas "
     [:code "get*"]
     " actually retrieves the element."]]

   "Vectors"
   [:div
    [:p "Comments for vectors..."]]}


  :chart-settings
  {"(fn [n] (dissoc-in* (nested-list n) (repeat n (dec n))))"
   {:x-axis-logarithmic? false
    :y-axis-logarithmic? false}

   "(fn [n] (dissoc-in* (nested-map n) (repeat n 0)))"
   {:x-axis-logarithmic? false
    :y-axis-logarithmic? false}

   "(fn [n] (dissoc-in* (nested-seq n) (repeat n (dec n))))"
   {:x-axis-logarithmic? false
    :y-axis-logarithmic? false}

   "(fn [n] (dissoc-in* (narrow-deep-vec n) (concat (repeat n-levels n) [(dec n)])))"
   {:x-axis-logarithmic? true
    :y-axis-logarithmic? true}

   "(fn [n] (dissoc-in* (nested-vec n) (repeat n (dec n))))"
   {:x-axis-logarithmic? false
    :y-axis-logarithmic? false}}}
